#Stepwise Model
#init population and probabilities
S <-  50000
I <-  20
R <-  0
N = S + I + R
Pspread <-  0.4
Pcontact <-  0.6
Precover <-  0.07
minUtil <- 20
#sets up the matrix
mod <- matrix(NA, nrow = N, ncol = 4)
colnames(mod) <- c("Step","Isolation", "Isolation Counter", "Utility")
#Step: S = 1, I = 2, R = 3
#Isolate: 1 = True, 0 = False
#sets up SIR
mod[1:S,1] <- 1
mod[(S+1):(S+I), 1] <- 2
if(S + I + 1 <= S + I + R){
mod[(S + I + 1):(S + I + R),1] <- 3
}
#sets up isolation
# 0.05 of S isolate
# 0.1 of I isolate
mod[mod[,"Step"] == 1,"Isolation"] <- rbern(nrow(mod[mod[,"Step"] == 1,]), 0.01)
mod[mod[,"Step"] == 2,"Isolation"] <- rbern(nrow(mod[mod[,"Step"] == 2,]), 0.1)
mod[mod[,"Step"] == 3,"Isolation"] <- 0
mod[,"Isolation Counter"] <- 0
#sets up utility
mod[,"Utility"] <- rnorm(n = N,mean = 50,sd = 50/3)
#display test
head(mod)
#sets up data frame
SIRdf <- data.frame("Step" = 0, "S" = S, "I" = I, "R" = R)
SIRdf
mod[mod[,"Step"] == 2, "Step"] + rbern(length(mod[mod[,"Step"] == 2, "Step"]), Precover)
#transitions
steps = 200
#utility update function
f <- function(x) rnorm(n = 1, mean = x, sd = 2)
for(i in 1:steps){
#Utility is updated for all stages
mod[,"Utility"] <- mapply(mod[,"Utility"], FUN = f)
#increase the day count for Isolation Counter + leaves isolation after 14 days
mod[mod[,"Isolation"] == 1, "Isolation Counter"] = mod[mod[,"Isolation"] == 1, "Isolation Counter"] + 1
mod[mod[,"Isolation Counter"] >= 14, "Isolation"] = 0
mod[mod[,"Isolation Counter"] >= 14, "Isolation Counter"] = 0
# S to I
#infected goes into isolation when their utility function goes below 3sd
#mod[mod[,"Step"] == 1 & mod[,"Isolation"] == 0 & mod[,"Utility"] < 50 - 3 * 50/3, "Isolation"] <- 1
#Transition from S to I
# The to meet is sum(infected not isolated + utility > 50/3*2)/sum(not isolating + utility > 20)
# Look at People that are not isolating and calculating what the probability of them meeting is
Pmeet <- length(mod[(mod[,"Step"] == 2 & mod[,"Isolation"] == 0 & mod[,"Utility"] >= minUtil) , "Step"])/
nrow(mod[mod[,"Utility"] > minUtil & mod[,"Isolation"] == 0,])
transition <- rbern(length(mod[(mod[,"Step"] == 1 & mod[,"Isolation"] == 0 & mod[,"Utility"] >= minUtil) , "Step"]), Pspread * Pmeet)
summary(transition)
mod[(mod[,"Step"] == 1 & mod[,"Isolation"] == 0 & mod[,"Utility"] >= minUtil) , "Step"] <- mod[(mod[,"Step"] == 1 & mod[,"Isolation"] == 0 & mod[,"Utility"] >= minUtil) , "Step"]  +  transition
# I to R
# Stationary Transition rate Precover
mod[mod[,"Step"] == 2, "Step"] <- mod[mod[,"Step"] == 2, "Step"]  +
rbern(length(mod[mod[,"Step"] == 2, "Step"]), Precover)
#Sends a proportion of Infected to Isolation (Utility < 1 sd)
mod[mod[,"Step"] == 2 & mod[,"Utility"] < 50 - 50/3, c("Isolation","Isolation Counter")] <- c(1,1)
#updates data frame
newS <- nrow(mod[mod[,"Step"] == 1,])
newI <- nrow(mod[mod[,"Step"] == 2,])
newR <- nrow(mod[mod[,"Step"] == 3,])
SIRdf <- rbind(SIRdf,c(i,newS,newI,newR))
}
